    <div class="container">
      <form method="post" action="/admin/subcategoryData">
        <h4 class="display-20"> Category Info </h4>
        <!-- TEXTB0X: FIRST NAME -->
        <label for="id">category name</label> <br>
        <div class="form-group d-flex">
          <select class="form-control col-sm-4" name="category_name">
            <option value="">---select category---</option>
            <% for(let i in subcate) { %>
                <option value="<%= subcate[i]._id %>">
                    <%= subcate[i].category_name %>
                </option>
            <% } %>
        </select>
        </div>

        <label for="id">Sub_category name</label> <br>
        <div class="form-group d-flex">
          <input class="form-control col-sm-4" type="text" name="sub_category_name" placeholder="Category name">
        </div>
        <!-- BUTTON: SUBMIT -->
        <button class="btn btn-primary mb-5" type="submin" value="submin">
          Submit
        </button>

      </form>
      <br>

      <a class="btn btn-info mb-5" href="/admin/">add</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <a class="btn btn-info mb-5" href="/admin/category">category</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
